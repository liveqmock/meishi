<?xml version="1.0" encoding="UTF-8"?>
<ehcache updateCheck="false" name="ehcache">

	<diskStore path="java.io.tmpdir/ehcache/aiwan/front" />

	<!-- DefaultCache setting. -->
	<defaultCache maxEntriesLocalHeap="10000" eternal="true" timeToIdleSeconds="300" timeToLiveSeconds="600" overflowToDisk="true" maxEntriesLocalDisk="100000" />

	<cache name="tenantCache" maxElementsInMemory="1000" eternal="true" overflowToDisk="false" />
	<cache name="userCache" maxElementsInMemory="10000" eternal="true" overflowToDisk="false" />
	<cache name="configCache" maxElementsInMemory="10000" eternal="true" overflowToDisk="false" />
		
</ehcache>